<?xml version="1.0" encoding="UTF-8" ?>
<launch>

  <arg name="map_file" default="$(find ros_motionplanning)/maps/gmapping-005.yaml"/>
  <node pkg="ros_motionplanning" type="ros_motionplanning_node" name="astar" output="screen">
  </node>
  <!-- Run the map server -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

</launch>


